<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="155.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controllers.AddNewPackController">
    <children>
        <TextField fx:id="packNameField" layoutX="14.0" layoutY="35.0" promptText="Назва упаковки" />
        <Button fx:id="addNewSort" layoutX="255.0" layoutY="35.0" mnemonicParsing="false" onAction="#onAddNewSort" prefHeight="25.0" prefWidth="149.0" text="Додати нову упаковку" />
        <Button fx:id="back" layoutX="352.0" layoutY="99.0" mnemonicParsing="false" onAction="#onBack" text="Назад" />
        <Label layoutX="14.0" layoutY="14.0" text="Введіть назву упавоки" />
      <TextField fx:id="packVolumeField" layoutX="14.0" layoutY="112.0" promptText="Об'єм упаковки" />
      <Label layoutX="14.0" layoutY="91.0" text="Введіть об'єм упаковки" />
    </children>
</AnchorPane>
