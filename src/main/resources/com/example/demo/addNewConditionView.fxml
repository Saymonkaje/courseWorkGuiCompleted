<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="529.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controllers.AddNewConditionController">
   <children>
      <Button fx:id="addNewCondition" layoutX="299.0" layoutY="78.0" mnemonicParsing="false" onAction="#onAddNewCondition" text="Додати новий фізичний стан кави" />
      <TextField fx:id="textField" layoutX="36.0" layoutY="78.0" promptText="Введіть фізичний стан" />
      <Button fx:id="back" layoutX="402.0" layoutY="141.0" mnemonicParsing="false" onAction="#onBack" prefHeight="25.0" prefWidth="65.0" text="Назад" />
      <Label layoutX="50.0" layoutY="53.0" text="Введіть фізичний стан" />
   </children>
</AnchorPane>
